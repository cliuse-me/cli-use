{
  "name": "@cli-use/tui",
  "version": "0.1.20",
  "description": "Build beautiful terminal user interfaces with styled components - A powerful TUI framework for creating stunning CLI applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    }
  },
  "bin": {
    "cli-use": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "scripts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "prepublishOnly": "npm run build",
    "test:ai": "tsx scripts/test-ai.ts",
    "list:models": "tsx scripts/list-models.ts",
    "demo:cli-use": "tsx src/examples/cli-use-demo.tsx",
    "demo:trading": "tsx src/examples/trading-cli.tsx",
    "test": "npm run lint && npm run build",
    "release": "np"
  },
  "keywords": [
    "tui",
    "terminal",
    "cli",
    "command-line",
    "interface",
    "terminal-ui",
    "console",
    "styled-components",
    "beautiful-ui",
    "interactive",
    "components"
  ],
  "author": "Arthur Alves <arthurdata404@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cliuse-me/cli-use.git"
  },
  "bugs": {
    "url": "https://github.com/cliuse-me/cli-use/issues"
  },
  "homepage": "https://github.com/cliuse-me/cli-use#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.29",
    "@types/react-dom": "^19.2.3",
    "ai": "^6.0.86",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "dotenv": "^17.3.1",
    "eastasianwidth": "^0.3.0",
    "ink": "npm:@jrichman/ink@6.4.10",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-reconciler": "^0.29.2",
    "slice-ansi": "^8.0.0",
    "string-width": "^7.2.0",
    "wrap-ansi": "^9.0.0"
  },
  "overrides": {
    "ink-select-input": {
      "ink": "$ink"
    },
    "ink-spinner": {
      "ink": "$ink"
    },
    "ink-text-input": {
      "ink": "$ink"
    }
  },
  "devDependencies": {
    "@types/node": "^22.19.11",
    "@types/react": "^19.2.14",
    "@types/react-reconciler": "^0.28.8",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^17.3.0",
    "np": "^11.0.2",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
